def r2hyr1(r1,r2):
    r1hy=""
    if len(r1)>len(r2):
        lll=len(r2)
    else:
        lll=len(r1)
    for i in range(lll-1):
        if r1[i]=="T" and r2[i]=="C":
            r1hy=r1hy+"C"
        else:
            r1hy=r1hy+r1[i]
    return r1hy

a1="ATTTATATGTGTTTTACATTGTATTTGGAGTTATAGTTATATTATATTTAATACTTTGTATAATGTAAATTAAGATATTGTTGTTGTTATTTTGTGTAGTTTAGGGAGTAAGGAGAAGAAATAGTTGTTAGTTTGTGTTTAGTAAGGATA"
a2="ACCTACATGTGCCCCACACTGCACTTGGAGTCATAACGCCATTACCCACCCTATACCTATATATTCTACATCCTCACAATTAAACAAAATTAAAAATATTCAAACCAATTTTATCCTTACTAAACACAAACTAACAACTATTTCTTCTC"



ppp=r2hyr1(a1,a2)
print(ppp)
